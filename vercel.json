{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.py"
    },
    {
      "src": "/navada_dashboard.html",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/jobs",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*\\.(png|csv|js|css))",
      "dest": "/api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  }
}